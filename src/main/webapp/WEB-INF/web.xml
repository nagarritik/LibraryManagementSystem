<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>addStudent</servlet-name>
    <servlet-class>com.ud.lms.controller.addStudent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addStudent</servlet-name>
    <url-pattern>/addStudent</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>addBook</servlet-name>
    <servlet-class>com.ud.lms.controller.addBook</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addBook</servlet-name>
    <url-pattern>/addBook</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>issueBook</servlet-name>
    <servlet-class>com.ud.lms.controller.issueBook</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>issueBook</servlet-name>
    <url-pattern>/issueBook</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>returnBook</servlet-name>
    <servlet-class>com.ud.lms.controller.returnBook</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>returnBook</servlet-name>
    <url-pattern>/returnBook</url-pattern>
  </servlet-mapping>
</web-app>